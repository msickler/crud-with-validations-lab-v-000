<h1><%= @song.title %></h1>
<p>Released: <%= @song.released %></p>
<p>Release Year: <%= @song.release_year %></p>
<p>Artist: <%= @song.artist_name %></p>
<p>Genre: <%= @song.genre %> </p>

<h1> Edit Song </h1>
<%= form_for(@song) do |f| %>
<%= f.label :title %><br>
<%= f.text_field :title %><br>
<%= f.label :released %><br>
<%= f.text_field :released %><br>
<%= f.label :release_year %><br>
<%= f.text_field :release_year %><br>
<%= f.label :artist_name %><br>
<%= f.text_field :artist_name %><br>
<%= f.label :genre %><br>
<%= f.text_field :genre %><br>
<%= f.submit %>
<% end %>